{
  "name": "chessroyale",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "shared",
    "client",
    "server"
  ],
  "scripts": {
    "build": "yarn workspaces run build",
    "dev": "concurrently \"yarn workspace @chessroyale/client dev\" \"yarn workspace @chessroyale/server dev\"",
    "start": "yarn workspace @chessroyale/server start"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "^5.3.3",
    "@types/node": "^20.11.0"
  }
}
